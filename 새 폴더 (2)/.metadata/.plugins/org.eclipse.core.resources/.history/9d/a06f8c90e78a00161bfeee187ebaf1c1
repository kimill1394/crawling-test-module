package javaExample;

public class SimpleTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
					
		
		
		Data d = new Data();
		d.x = 10;
		change(d.x);
		System.out.println("After Change(): "+d.x);
		
		
	}
	
	
	static void change(int x) {
		x = 1000;
		
		System.out.println("change(): "+x);
	}

}



class Data {
	int x;
}